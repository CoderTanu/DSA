package patternPractices;

public class PatternPrint {
	
	
	/*****
	*****
	*****
	*****
	*****/
	public void squarePattern1(int n) {
		for(int i=0; i<n; i++) {
			for(int j=0;j<n; j++) {
				System.out.print("*");
			}
			System.out.println("");
		}
	}
	
	
	public  void nForest(int n) {
        for(int i=0;i<n; i++){
            for(int j=0; j<=i; j++){
                System.out.print("* ");
            }
             System.out.println("");
        }
    }
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	

}
