package pattenPractices;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		PattenPrint patten = new PattenPrint();
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		for(int i=0; i<t; i++) {
		int n=sc.nextInt();
		patten.squarePatten1(n);
	}
	}
}
