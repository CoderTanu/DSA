package pattenPractices;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		PattenPrint pattern = new PattenPrint();
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the test cases:");
		int t = sc.nextInt();
		System.out.println("Excute for" + t +"test cases:");
		for(int i=0; i<t; i++) {
		int n=sc.nextInt();
		System.out.println("Enter the pattern to print");
		pattern.squarePattern1(n);
	}
	}
}
